{
  "name": "ai-agent-example",
  "version": "1.0.0",
  "description": "AI Agent with modular tools, LLM abstraction, and history management",
  "main": "src/agent.ts",
  "type": "module",
  "scripts": {
    "start": "tsx --env-file .env src/agent.ts",
    "dev": "tsx watch src/agent.ts",
    "test": "tsx --test tests/*.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "ai",
    "claude",
    "agent",
    "file-operations",
    "tutorial"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.10.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.51.0"
  }
}
